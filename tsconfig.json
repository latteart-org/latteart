{
  "compilerOptions": {
    "target": "ES2021",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": true,
    "allowJs": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
