{
  "name": "latteart-gui",
  "version": "2.15.1",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:viewer": "run-p type-check \"build-only:viewer {@}\" --",
    "build:viewer:history": "run-p type-check \"build-only:history {@}\" --",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "build-only": "vite build",
    "build-only:viewer": "vite --config vite.config.viewer.ts build",
    "build-only:history": "vite --config vite.config.viewer.history.ts build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "package": "run-p build build:viewer build:viewer:history",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version"
  },
  "dependencies": {
    "chart.js": "^4.4.2",
    "d3": "^7.8.2",
    "dayjs": "^1.11.3",
    "mermaid": "^9.3.0",
    "pinia": "^2.1.7",
    "splitpanes": "^3.1.5",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.5.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.6",
    "@types/mermaid": "8.2.7",
    "@types/splitpanes": "^2.2.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuetify": "^2.2.0",
    "jsdom": "^24.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.72.0",
    "vite": "^5.1.5",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  }
}
