{
  "name": "latteart-gui",
  "version": "2.9.0",
  "private": true,
  "scripts": {
    "test": "vue-cli-service test:unit",
    "build": "vue-cli-service build",
    "build:viewer": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.viewer.js vue-cli-service build",
    "build:viewer:history": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.viewer.history.js vue-cli-service build",
    "serve": "vue-cli-service serve --port 3000",
    "package": "run-p build build:viewer build:viewer:history",
    "lint": "vue-cli-service lint src/**/*.ts src/**/*.vue",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "d3": "^7.8.2",
    "dayjs": "^1.11.3",
    "leaflet": "^1.5.1",
    "mermaid": "^9.1.2",
    "splitpanes": "^2.3.6",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.11.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vue2-leaflet": "^2.2.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.55",
    "@types/d3": "^7.4.0",
    "@types/leaflet": "^1.4.4",
    "@types/mermaid": "8.2.7",
    "@types/splitpanes": "^2.2.0",
    "@types/vuedraggable": "^2.24.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2"
  }
